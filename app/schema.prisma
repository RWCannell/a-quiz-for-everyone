model Option {
  id                Int      @id @default(autoincrement())
  optionText        String   @unique
  optionDescription Int
  questionId        Int
  question          Question @relation(fields: [questionId], references: [id])
}

model Question {
  id              Int      @id @default(autoincrement())
  questionText    String
  subject         String
  correctOptionId Int      @unique
  options         Option[]
}
